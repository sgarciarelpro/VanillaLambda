{
  "name": "vendors-list",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: restApi2",
  "customName": false,
  "customRole": false,
  "handler": "vendors/list/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [
    {
      "path": "vendors/list",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {
        "integration.request.header.userToken": "method.request.header.userToken"
      },
      "requestTemplates": {
        "application/json": "{\"body\": $input.json('$'),\"header\": {#foreach( $header in $input.params().header.keySet()) \"$header\": \"$util.escapeJavaScript($input.params().header.get($header))\" #if($foreach.hasNext), #end #end}}"
      },
      "responses": {
        "400": {
          "statusCode": "400"
        },
        "default": {
          "statusCode": "200",
          "responseParameters": {},
          "responseModels": {
            "application/json;charset=UTF-8": "Empty"
          },
          "responseTemplates": {
            "application/json;charset=UTF-8": ""
          }
        }
      }
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "relProBasePath": "${relProBasePath}",
    "relProHost": "${relProHost}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}